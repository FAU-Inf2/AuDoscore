package asp;
import java.util.*;
import java.lang.reflect.*;
import tester.*;

public class Config{
	public static void setReplace(Method oldm, Method newm){
		String sig = ReadReplace.getSig(oldm);
		int index = replacedMap.get(sig);
		replacedMethods[index] = newm;
	}
	public static void clearReplace(){
		for(int i=0; i<replacedMethods.length; ++i){
			replacedMethods[i] = null;
		}
	}
