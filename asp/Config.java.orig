package asp;
import java.util.*;
import java.lang.reflect.*;
import tester.*;

public class Config{
	public static void setReplace(java.lang.reflect.Method m){
		String sig = ReadReplace.getSig(m);
		int index = replacedMap.get(sig);
		replacedMethods[index] = m;
	}
	public static void clearReplace(){
		for(int i=0; i<replacedMethods.length; ++i){
			replacedMethods[i] = null;
		}
	}
